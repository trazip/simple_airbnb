<h1>Flats</h1>
<div class="cards">
  <% @flats.each do |flat| %>
    <div class="card-trip">
      <img src="<%= flat["imageUrl"] %>" />
      <div class="card-trip-infos">
        <div>
          <h2><%= flat["name"] %></h2>
          <p>Nice flat over there!</p>
        </div>
        <h2 class="card-trip-pricing"><%= flat["price"]%><%= flat["priceCurrency"]%></h2>
      </div>
    </div>
  <% end %>
</div>
